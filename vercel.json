{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://*.twilio.com https://*.twiliocdn.com"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/outbound-call",
      "methods": ["POST"],
      "dest": "/outbound.js"
    },
    {
      "src": "/outbound-call-twiml",
      "methods": ["GET", "POST"],
      "dest": "/outbound.js"
    },
    {
      "src": "/outbound-media-stream",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/outbound.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS,UPGRADE",
        "Access-Control-Allow-Headers": "*",
        "Access-Control-Allow-Credentials": "true",
        "Upgrade": "$http_upgrade",
        "Connection": "$connection_upgrade"
      }
    },
    {
      "src": "/twilio/inbound_call",
      "methods": ["GET", "POST"],
      "dest": "/index.js"
    },
    {
      "src": "/media-stream",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS,UPGRADE",
        "Access-Control-Allow-Headers": "*",
        "Access-Control-Allow-Credentials": "true",
        "Upgrade": "$http_upgrade",
        "Connection": "$connection_upgrade"
      }
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ]
}
